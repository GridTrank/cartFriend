}
.page-wrap .eva-input {
  position: fixed;
  background-color: #f2f2f2;
  padding: 40rpx;
  bottom: 0;
  z-index: 999;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.page-wrap .eva-input input {
  background-color: #fff;
  width: 80%;
  height: 70rpx;
  border-radius: 8rpx;
  padding-left: 10rpx;
}
.page-wrap .eva-input .foc {
  margin-bottom: 36rpx;
}
.page-wrap .is-focus input, .page-wrap .is-focus .send-eva {
  margin-bottom: 40rpx;
}
.page-wrap .friend-wrap {
  width: 100%;
  height: 80vh;
  padding: 30rpx;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
}
.page-wrap .friend-list {
  white-space: nowrap;
  background-color: #fff;
  border-radius: 12rpx;
}
.page-wrap .friend-list .item {
  position: relative;
  padding: 30rpx;
  border-bottom: 1px solid #f1f1f1;
}
.page-wrap .friend-list .item:last-child {
  border: none;
}
.page-wrap .friend-list .item .avatar {
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}
.page-wrap .friend-list .item .btn {
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background-color: #5FB800;
  color: #fff;
  font-size: 24rpx;
  border-radius: 12rpx;
  position: absolute;
  right: 30rpx;
  top: 50rpx;
}
.page-wrap .tips {
  width: 100%;
  padding: 20rpx 0;
  text-align: center;
  color: #666;
  font-size: 28rpx;
  align-items: center;
  margin-top: 120rpx;
}
.page-wrap .tips image {
  width: 280rpx;
  display: block;
}

