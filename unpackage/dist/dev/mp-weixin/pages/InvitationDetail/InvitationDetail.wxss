.page-wrap {
  padding-bottom: 200rpx;
}
.page-wrap .swiper-wrap {
  position: relative;
  min-height: 60rpx;
  background-color: #fff;
}
.page-wrap .swiper-wrap .video {
  width: 100%;
}
.page-wrap .swiper-wrap .more {
  position: absolute;
  z-index: 9999;
  background-color: #f59a23;
  padding: 10rpx 30rpx;
  border-radius: 50rpx;
  color: #fff;
  top: 30rpx;
  right: 64rpx;
}
.page-wrap .shunlujia {
  padding: 0 30rpx;
}
.page-wrap .shunlujia .sm-img {
  width: 100%;
  height: 120rpx;
}
.page-wrap .shunlujia .info .type {
  width: 90%;
  background-color: #fff;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
  text-align: center;
  color: #412815;
  font-size: 32rpx;
  padding: 16rpx 0;
  margin: auto;
}
.page-wrap .shunlujia .info .address {
  background: linear-gradient(359deg, #DFAD76 0%, #FFECD8 100%);
  border-radius: 20rpx;
  padding: 30rpx;
  justify-content: center;
  color: #000;
}
.page-wrap .shunlujia .info .address .ad {
  width: 40%;
  text-align: center;
}
.page-wrap .shunlujia .info .address .t1 {
  font-size: 28rpx;
}
.page-wrap .shunlujia .info .address .t2 {
  font-size: 36rpx;
}
.page-wrap .shunlujia .info .address .jh {
  width: 32rpx;
  height: 32rpx;
  margin: 0 30rpx;
}
.page-wrap .shunlujia .phone .p-num {
  font-size: 24rpx;
  color: #000;
}
.page-wrap .shunlujia .phone .p-sw {
  background-color: #F5F2FF;
  border-radius: 24rpx;
  padding: 8rpx 22rpx;
  color: #0A2463;
  font-size: 24rpx;
}
.page-wrap .shunlujia .phone .p-sw image {
  width: 32rpx;
  height: 32rpx;
  margin-left: 10rpx;
}
.page-wrap .shunlujia .phone .p-val {
  color: #000000;
  font-size: 40rpx;
}
.page-wrap .shunlujia .phone .vip {
  width: 260rpx;
  height: 60rpx;
}
.page-wrap .shunlujia .time .val {
  color: #000;
  font-size: 40rpx;
}
.page-wrap .shunlujia .img-wrap {
  display: flex;
  flex-wrap: nowrap;
  white-space: nowrap;
}
.page-wrap .shunlujia .img-wrap image {
  flex-shrink: 0;
  width: 200rpx;
  height: 200rpx;
  border-radius: 18rpx;
  margin-right: 30rpx;
}
.page-wrap .content-wrap {
  padding: 30rpx;
}
.page-wrap .content {
  background-color: #fff;
  padding: 40rpx;
}
.page-wrap .content .user-info {
  justify-content: space-between;
}
.page-wrap .content .user-info .user .user-img {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.page-wrap .content .user-info .zan .z-left {
  margin-left: 10rpx;
}
.page-wrap .content .txt {
  font-size: 30rpx;
  color: #000;
  line-height: 50rpx;
}
.page-wrap .content .jiesuan .close {
  background-color: #F7D8B6;
  color: #8F5E29;
  font-size: 24rpx;
  padding: 4rpx 16rpx;
  border-radius: 12rpx;
}
.page-wrap .content .jiesuan .ing {
  color: #8F5E29;
}
.page-wrap .evaluate {
  padding: 40rpx;
  background-color: #fff;
  min-height: 54h;
}
.page-wrap .evaluate .top {
  justify-content: space-between;
}
.page-wrap .evaluate .top .all {
  color: #5FB800;
  font-size: 28rpx;
  position: relative;
}
.page-wrap .evaluate .top .all::after {
  content: "";
  display: block;
  position: absolute;
  width: 100%;
  height: 4rpx;
  background-color: #5FB800;
  border-radius: 4rpx;
  bottom: -20rpx;
}
.page-wrap .evaluate .eva-item .eva-list {
  align-items: flex-start;
}
.page-wrap .evaluate .eva-item .eva-list .eva-img {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.page-wrap .evaluate .eva-item .eva-list .eva-info {
  width: 520rpx;
}
.page-wrap .evaluate .eva-item .eva-list .eva-info .eva-model {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.page-wrap .evaluate .eva-item .eva-list .eva-info .eva-model .time {
  font-size: 24rpx;
  color: #666;
}
.page-wrap .evaluate .eva-item .eva-list .eva-info .eva-model .num-s {
  margin-right: 20rpx;
  display: flex;
  align-items: center;
  color: #676D82;
  font-size: 28rpx;
}
.page-wrap .evaluate .eva-item .eva-list .eva-info .detail-list {
  padding: 30rpx;
  background-color: rgba(0, 0, 0, 0.1);
  border-radius: 20rpx;
}
.page-wrap .evaluate .eva-item .eva-list .eva-info .detail-list .detail-item .t1 {
  font-size: 30rpx;
}
.page-wrap .evaluate .eva-item .show-more {
  color: #f59a23;
  font-size: 32rpx;
}
.page-wrap .evaluate .u-input {
  background-color: #fff;
  width: 150rpx;
}
.page-wrap .more-list {
  background-color: #fff;
  border-radius: 8rpx;
  padding: 40rpx 0;
}
.page-wrap .more-list .list {
  flex-direction: column;
  padding: 40rpx;
}
.page-wrap .more-list .list .item {
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 50rpx;
  margin-bottom: 20rpx;
  width: 100%;
  background-color: #f59a23;
  color: #fff;
  text-align: center;
}
.page-wrap .foot-tool {
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: #fff;
  z-index: 888;
}
.page-wrap .foot-tool .foot-list {
  justify-content: space-around;
  padding: 40rpx 0;
}
.page-wrap .foot-tool .foot-list .foot-item {
  border: none;
  background-color: #f2f2f2;
}
.page-wrap .foot-tool .foot-list .foot-item .u-btn {
  border: none;
  padding: 0 20rpx;
}
.page-wrap .foot-tool .foot-list .foot-item .u-size-default {
  height: auto;
}
.page-wrap .foot-tool .foot-list .foot-item button::after {
  border: none;
}
.page-wrap .foot-tool .foot-list .foot-item .tool-item {
  align-items: center;
  color: #676D82;
  font-size: 28rpx;
}
.page-wrap .foot-tool .foot-list .foot-item .tool-item .tool-img {
  width: 50rpx;
}
.page-wrap .foot-tool .foot-list .foot-item .t1 {
