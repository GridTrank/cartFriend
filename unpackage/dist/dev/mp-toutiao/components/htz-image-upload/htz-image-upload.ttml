<view class="htz-image-upload-list"><block tt:for="{{$root.l0}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="htz-image-upload-Item"><block tt:if="{{!item.g0}}"><view class="htz-image-upload-Item-video"><video disabled="{{false}}" controls="{{false}}" src="{{item.$orig}}"><cover-view data-event-opts="{{[['tap',[['previewVideo',['$0'],[[['uploadLists','',index]]]]]]]}}" class="htz-image-upload-Item-video-fixed" bindtap="__e"></cover-view><block tt:if="{{remove&&previewVideoSrc==''}}"><cover-view data-event-opts="{{[['tap',[['imgDel',[index]]]]]}}" class="htz-image-upload-Item-del-cover" bindtap="__e">×</cover-view></block></video></view></block><block tt:else><image src="{{item.$orig}}" data-event-opts="{{[['tap',[['imgPreview',['$0'],[[['uploadLists','',index]]]]]]]}}" bindtap="__e"></image></block><block tt:if="{{remove}}"><view data-event-opts="{{[['tap',[['imgDel',[index]]]]]}}" class="htz-image-upload-Item-del" bindtap="__e">×</view></block></view></block><block tt:if="{{uploadLists.length<max}}"><view data-event-opts="{{[['tap',[['chooseFile',['$event']]]]]}}" class="htz-image-upload-Item htz-image-upload-Item-add" bindtap="__e">+</view></block><block tt:if="{{previewVideoSrc!=''}}"><view class="preview-full"><video autoplay="{{true}}" src="{{previewVideoSrc}}" show-fullscreen-btn="{{false}}"><cover-view data-event-opts="{{[['tap',[['previewVideoClose',['$event']]]]]}}" class="preview-full-close" bindtap="__e">×</cover-view></video></view></block></view>